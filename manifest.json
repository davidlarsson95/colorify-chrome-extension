{
  "manifest_version": 2,

  "name": "Colorify",
  "description": "Color tool for assistance with colors when browsing!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
